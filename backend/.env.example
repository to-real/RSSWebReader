# Database URL (SQLite for development, PostgreSQL for production)
DATABASE_URL=sqlite:///./rss.db

# AI Provider: "claude" or "zhipu"
AI_PROVIDER=zhipu

# Claude API Key (if using Claude)
CLAUDE_API_KEY=your-claude-api-key-here

# Zhipu AI API Key (if using Zhipu/BigModel)
ZHIPU_API_KEY=your-zhipu-api-key-here

# Feed fetch interval in minutes
FETCH_INTERVAL_MINUTES=30

# Claude API concurrency settings
CLAUDE_MAX_CONCURRENCY=3
CLAUDE_MAX_CONTENT_LENGTH=3000

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Sentry DSN for error tracking (optional)
SENTRY_DSN=
